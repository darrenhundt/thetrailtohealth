
<div class="gallery-wrapper" data-controller="GalleryLayout">
  <div class="slides">
    <div class="grid-wrapper">
      {.repeated section items}
      <hr/>
          {title}
          <br/>
          {body}
          <br/>
          {assetUrl}
          <br/>
          {oembed.width}
      <br/>
      <a href="{oembed.url}" data-lity>{title}</a>
      <hr/>
          
      {.end}
    </div>
  </div>
</div>

