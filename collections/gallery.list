
<div class="gallery-wrapper" data-controller="GalleryLayout">
  <div class="slides">
    <div class="grid-wrapper">
      {.repeated section items}
      <hr/>
          {title}
          <br/>
          {body}
          <br/>
          {assetUrl}
          <br/>
          {oembed.width}
      <hr/>
      
      <div id="container">
        <center>
          <button id="playme" onclick="revealVideo('video','youtube')"><img src="{oembed.thumbnailUrl}"/></button>
        </center>
      </div>

      <div id="video" class="lightbox" onclick="hideVideo('video','youtube')">
        <div class="lightbox-container">  
          <div class="lightbox-content">

            <button onclick="hideVideo('video','youtube')" class="lightbox-close">
              Close | âœ•
            </button>
            <div class="video-container">
              <iframe id="youtube" width="960" height="540" src="https://www.youtube.com/embed/WsptdUFthWI?showinfo=0" frameborder="0" allowfullscreen></iframe>
            </div>      

          </div>
        </div>
      </div>
          
          
      {.end}
    </div>
  </div>
</div>